#!/usr/bin/env python2

from __future__ import absolute_import, division, print_function
from functools import partial

import nfldb

import nflfan

db = nfldb.connect()
conf = nflfan.config()
player_search = partial(nflfan.player_search, db)

lg = conf['yahoo'][0]
prov = nflfan.provider.Yahoo(lg)

owners = prov.owners()
roster = prov.roster(player_search, owners[7], 2)
scored = nflfan.score_roster(roster.provider.scoring, roster)
print(scored)

